{
  "name": "mohen-vueuse",
  "version": "1.0.1",
  "description": "个人封装的vue3公共方法",
  "umd:main": "lib/index.umd.js",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "scripts": {
    "build:M": "rm -rf lib && microbundle",
    "dev": "eslint --ext .js,.jsx,.vue,.ts,.tsx src && vite",
    "build": "eslint --ext .js,.jsx,.vue,.ts,.tsx src && vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src",
    "prepare": "husky install",
    "test": "npx jest"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "@types/jest": "^27.0.1",
    "ts-jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "microbundle": "^0.13.3",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": "eslint --fix"
  }
}
